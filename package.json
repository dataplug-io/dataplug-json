{
  "name": "@dataplug/dataplug-json",
  "version": "0.11.0",
  "description": "Dataplug JSON library (by Dataplug.io)",
  "main": "lib/index.ts",
  "scripts": {
    "jsdoc": "jsdoc --destination ./docs --recurse ./lib --verbose",
    "lint": "standard",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls",
    "preversion": "yarn run lint && yarn run test",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": "10.x"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectDirectory": "./tests/coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dataplug-io/dataplug-json.git"
  },
  "author": "",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/dataplug-io/dataplug-json/issues"
  },
  "homepage": "https://github.com/dataplug-io/dataplug-json#readme",
  "dependencies": {
    "@types/bluebird": "^3.5.1",
    "@types/check-types": "^7.3.0",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.5",
    "jest": "^24.7.1",
    "oboe": "^2.1.4",
    "prettier": "^1.17.0",
    "ts-stream": "^2.0.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.3",
    "jsdoc": "^3.5.5",
    "standard": "^10.0.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.3"
  }
}
